<BorderPane prefHeight="500.0" prefWidth="853.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TabletController">
   <left>
      <AnchorPane prefHeight="500.0" prefWidth="106.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="500.0" prefWidth="53.0" AnchorPane.leftAnchor="53.0">
               <children>
                  <Button fx:id="mainPageBtn" maxHeight="53.0" maxWidth="53.0" minHeight="53.0" minWidth="53.0" mnemonicParsing="false" onAction="#mainPageClick" prefHeight="53.0" prefWidth="53.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="addFavBtn" mnemonicParsing="false" onAction="#favViewClick" prefHeight="53.0" prefWidth="53.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="addRecipeBtn" mnemonicParsing="false" onAction="#addViewClick" prefHeight="53.0" prefWidth="53.0">
                     <VBox.margin>
                        <Insets top="340.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </left>
   <center>
      <Label text="Welcome!" BorderPane.alignment="CENTER">
